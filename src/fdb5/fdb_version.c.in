#include "fdb_version.h"

const char * fdb_version()     { return FDB_VERSION; }
const char * fdb_version_str() { return FDB_VERSION_STR; }

unsigned int fdb_version_int()
{
  return 10000*FDB_MAJOR_VERSION + 100*FDB_MINOR_VERSION + 1*FDB_PATCH_VERSION;
}

const char * fdb_git_sha1() { return "@FDB_GIT_SHA1@"; }
