#!/bin/bash

if [[ "$MULTIO_SINKS" == "fdb5" ]]
then
    CMD=/home/ma/tmpfdb/bin/grib2fdb5
else
    if [[ -x /usr/local/apps/mars/current/GNU/48/bin/grib2fdb ]]
    then
        CMD=/usr/local/apps/mars/current/GNU/48/bin/grib2fdb5
    else
        CMD=/usr/local/lib/metaps/mars_grib_api/bin/grib2fdb
    fi
fi

exec $CMD "$@"
